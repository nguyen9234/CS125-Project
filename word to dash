char dash(char a)
{
    srand(time(NULL));
    char* word;
    *word = a;
    printf("%s", word);
    
    int len, x;
    len = strlen(word)-1;
    for(x=0; x<len; x++)
    {
        printf("_ ");
    }
    
}



//word to dash alternate version?
//try to put into a txt file, like the word selectors
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#include <ctype.h>

char* dash(char a)
{
    //srand(time(NULL));
    char* word;
    *word = a;
    //printf("%s", word);
    
    int len, x;
    len = strlen(word)-1;
    char* null_word[20]; 
    
    for(x=0; x<len; x++)
    {
        null_word[x] = "_";
    }

    return null_word;
}



#include <stdio.h>
#include <stdlib.h>
#include <string.h>


char* dash(char *a)
{

    
    int len, x;
    len = strlen(a)-1;
    char* null_word = malloc(20); 
    
    for(x=0; x<len; x++)
    {
        null_word[x] = '_';
    }
    printf("\n");
    
    return null_word;
}
